void main() {
  String nome = 'Rogério';

  
  if(nome = 'Rogério'){
    print('$nome, vocẽ é uma criança');
  } 
  else if(idade <= 17){
    print('$nome, vocẽ é um adolescente');
  } 
  else if(idade <= 59){
    print('$nome, vocẽ é um adulto');
  } 
  else if(idade >= 60 && idade <= 99){
    print('$nome, vocẽ é um idoso');
  }
  else if(idade >= 100){
    print('DIGITE UM VALOR VÁLIDO.');
  }
  }
